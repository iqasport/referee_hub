  "eslint-junit": {
    "suiteName": "ES Lint",
    "output": "./eslint-junit.xml",
    "classNameTemplate": "{ruleId}",
    "titleTemplate": "{filePath} {line}:{column} {message}"
  },
  "scripts": {
    "test": "jest -c jest.config.js --coverage",
    "test:ci": "jest --ci --runInBand --coverage --config jest.config.js --reporters=jest-junit",
    "lint": "eslint .",
    "lint:ci": "eslint -f ./node_modules/eslint-junit/index.js .",
    "swaggergen": "rtk-query-codegen-openapi openapi-config.js",
    "start:dev": "webpack serve --config webpack.config.dev.js",
    "build:dev": "yarn build:app:dev && yarn styles && yarn images:copy",
    "build:prod": "yarn build:app:prod && yarn styles && yarn images:copy",
    "build:app:dev": "webpack --config webpack.config.dev.js",
    "build:app:prod": "webpack --config webpack.config.prod.js",
    "styles": "yarn styles:common && yarn styles:app",
    "styles:common": "postcss ./assets/stylesheets/application.css -o dist/management_hub.css",
    "styles:app": "sass ./app/assets/stylesheets/app.scss dist/management_hub_app.css",
    "images:copy": "shx cp -R ./assets/images ./dist/"
  }
